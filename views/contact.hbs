		<br>
		<br>
		<div class="row">
			<div class="col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1">
	      <div class="panel panel-default" style="color: black;">
	        <div class="panel-heading">
	          <h4>
	            Leaderboard
	          </h4>
	        </div>
	        <table class="table table-fixed">
	          <thead>
	            <tr>
	              <th>#</th><th>User Name</th><th>Phone</th><th>email</th>
	            </tr>
	          </thead>
	          <tbody>
							{{#each users}}
                <tr>
									<th scope="row">&nbsp;{{@index}}</th>
									{{#if this.uname}}
										<td>{{this.uname}}</td>
									{{else}}
										<td>{{this.emailId}}</td>
									{{/if}}
									<td>{{this.phone}}</td>
                  <td>{{this.emailId}}</td>
                </tr>
							{{/each}}
	          </tbody>
	        </table>
	      </div>
			</div>
	  </div>
